{"ast":null,"code":"import FloatingHeader from \"./components/floating-header.vue\";\nimport HomeSection from \"./sections/home.vue\";\nimport AboutSection from \"./sections/about-me.vue\";\nimport { onMounted, onBeforeUnmount } from \"vue\";\n// import Footer from \"./components/footer.vue\";\n\nexport default {\n  __name: 'App',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const home = ref(null);\n    const aboutMe = ref(null);\n    const views = [\"HOME\", \"ABOUT-ME\", \"PROJECTS\", \"CONTACT-ME\"];\n    function handleScroll(event) {\n      event.preventDefault();\n      console.log(\"scrolling \", event);\n    }\n    onMounted(() => {\n      window.addEventListener(\"wheel\", handleScroll, {\n        passive: false\n      });\n    });\n    onBeforeUnmount(() => {\n      window.removeEventListener(\"wheel\", handleScroll);\n    });\n    const __returned__ = {\n      home,\n      aboutMe,\n      views,\n      handleScroll,\n      FloatingHeader,\n      HomeSection,\n      AboutSection,\n      onMounted,\n      onBeforeUnmount\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["FloatingHeader","HomeSection","AboutSection","onMounted","onBeforeUnmount","home","ref","aboutMe","views","handleScroll","event","preventDefault","console","log","window","addEventListener","passive","removeEventListener"],"sources":["/Users/ijeriomitogun/Projects/TypescriptProjects/portfolio-site-2.0/src/App.vue"],"sourcesContent":["<template>\n  <div>\n    <div class=\"content-grid\">\n      <floating-header class=\"header\" ref=\"header\"></floating-header>\n      <div class=\"title-background\">\n        <img src=\"@/assets/matrix-background.svg\" />\n      </div>\n      <div id=\"HOME\" ref=\"home\" class=\"content-block home-section\">\n        <home-section></home-section>\n      </div>\n      <div class=\"gradient-background\"></div>\n      <div id=\"ABOUT-ME\" ref=\"aboutMe\" class=\"content-block about-section\">\n        <about-section></about-section>\n      </div>\n      <div id=\"PROJECTS\" class=\"content-block project-section\">\n        <!-- <projects-section class=\"section-wrapper\"></projects-section> -->\n      </div>\n      <div class=\"flag-background\"></div>\n      <div id=\"CONTACT-ME\" class=\"content-block contact-section\">\n        <!-- <contact-me class=\"section-wrapper\"></contact-me> -->\n      </div>\n      <div class=\"footer\"></div>\n      <!-- <div class=\"content-block\" style=\"grid-row: 6/6\">\n        <footer-section class=\"section-wrapper\"></footer-section>\n      </div> -->\n    </div>\n    <!-- <div class=\"social-bar\"></div> -->\n  </div>\n</template>\n<script setup>\nimport FloatingHeader from \"./components/floating-header.vue\";\nimport HomeSection from \"./sections/home.vue\";\nimport AboutSection from \"./sections/about-me.vue\";\nimport { onMounted, onBeforeUnmount } from \"vue\";\n// import Footer from \"./components/footer.vue\";\n\nconst home = ref(null);\nconst aboutMe = ref(null);\nconst views = [\"HOME\", \"ABOUT-ME\", \"PROJECTS\", \"CONTACT-ME\"];\nfunction handleScroll(event) {\n  event.preventDefault();\n\n  console.log(\"scrolling \", event);\n}\nonMounted(() => {\n  window.addEventListener(\"wheel\", handleScroll, { passive: false });\n});\n\nonBeforeUnmount(() => {\n  window.removeEventListener(\"wheel\", handleScroll);\n});\n</script>\n<style lang=\"scss\">\n@import \"@/scss/variables.scss\";\n@import \"@/scss/styles.scss\";\n\n$header-height: 100px;\n$title-height: 650px;\n$aboutMe-height: 1700px;\n$projects-height: 1600px;\n$contactMe-height: 1000px;\n$footer-height: 500px;\n\n$page-height: $title-height + $aboutMe-height + $projects-height +\n  $contactMe-height + $footer-height;\n\n#app {\n  height: $page-height;\n  width: 100vw;\n  position: relative;\n}\n\n.content-grid {\n  position: relative;\n  height: 100%;\n  