tom: #f1f1f1 solid 15px;\n}\n\n@media screen and (max-width: $phone-screen-width) {\n  .content-grid {\n    position: relative;\n    // top: 60px;\n    height: 100%;\n    width: 100vw;\n    display: grid;\n    grid-template-columns: 5% 90% 5%;\n    grid-template-rows:\n      $title-height/2\n      $aboutMe-height\n      $projects-height\n      $contactMe-height\n      $footer-height;\n  }\n}\nbody::-webkit-scrollbar {\n  display: none;\n}\n.gear-image {\n  width: 30vw;\n  height: 30vw;\n  align-self: flex-start;\n  left: -16vw;\n  position: absolute;\n}\n</style>\n"],"mappings":";OAKaA,UAAoC;;EAJ1CC,EAAE,EAAC;AAAK;;EACNC,KAAK,EAAC;AAAc;gCAEvBC,mBAAA,CAEM;EAFDD,KAAK,EAAC;AAAkB,I,aAC3BC,mBAAA,CAA4C;EAAvCC,GAAoC,EAApCJ;AAAoC,G;;EAEtCC,EAAE,EAAC,MAAM;EAACC,KAAK,EAAC;;gCAGrBC,mBAAA,CAGO;EAFLE,KAA8C,EAA9C;IAAA;IAAA;EAAA,CAA8C;EAC9CH,KAAK,EAAC;;gCAERC,mBAAA,CAEM;EAFDF,EAAE,EAAC,UAAU;EAACC,KAAK,EAAC,eAAe;EAACG,KAAqB,EAArB;IAAA;EAAA;;gCAGzCF,mBAAA,CAEM;EAFDF,EAAE,EAAC,UAAU;EAACC,KAAK,EAAC,eAAe;EAACG,KAAqB,EAArB;IAAA;EAAA;;gCAGzCF,mBAAA,CAeM;EAdJF,EAAE,EAAC,YAAY;EACfC,KAAK,EAAC,eAAe;EACrBG,KAA+C,EAA/C;IAAA;IAAA;EAAA;;;;;uBAtBNC,mBAAA,CAyCM,OAzCNC,UAyCM,GAxCJJ,mBAAA,CAsCM,OAtCNK,UAsCM,GArCJC,YAAA,CAA+DC,0BAAA;IAA9CR,KAAK,EAAC,QAAQ;IAACS,GAAG,EAAC;iBACpCC,UAEM,EACNT,mBAAA,CAEM,OAFNU,UAEM,GADJJ,YAAA,CAA6BK,uBAAA,E,GAE/BC,UAGO,EACPC,UAEM,EACNC,UAEM,EACNC,UAeM,C","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}