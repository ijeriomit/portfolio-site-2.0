lide.name }}</h3>\n          </div>\n          <p>\n            {{ slide.text }}\n          </p>\n        </div>\n      </Slide>\n    </Carousel>\n  </div>\n</template>\n<script setup>\nimport { ref } from \"vue\";\nimport { store } from \"@/data.js\";\nimport { Carousel, Slide } from \"vue3-carousel\";\nimport \"vue3-carousel/dist/carousel.css\";\n\nconst slides = ref(store.testimonials);\nconst autoplay = ref(5000);\n// function moveSlides(dir) {\n//   console.log(\"direction: \", dir);\n//   if (dir == -1) {\n//     shiftSlidesLeft();\n//   } else if (dir == 1) {\n//     shiftSlidesRight();\n//   }\n// }\n\n// function slideClicked(slideIndex) {\n//   if (slideIndex > centerIndex.value) {\n//     moveSlides(1);\n//   } else if (slideIndex < centerIndex.value) {\n//     moveSlides(-1);\n//   }\n// }\n</script>\n<style lang=\"scss\" scoped>\n@import \"@/scss/variables.scss\";\n$testimonial-width: 275px;\n$testimonial-height: 275px;\n$testimonials-gap: 50px;\n$testimonial-padding: 20px;\n$active-testimonial-padding: 30px;\n$testimonials-width: calc(\n  $testimonial-width * 3 + ($testimonials-gap * 2) + ($testimonial-padding * 4) +\n    ($active-testimonial-padding * 2)\n);\n.carousel__item {\n  min-height: 200px;\n  width: 100%;\n}\n\n.carousel__slide {\n  padding: 10px;\n}\n\n.carousel__prev,\n.carousel__next {\n  box-sizing: content-box;\n  border: 5px solid white;\n}\n\n.wrapper {\n  display: flex;\n  flex-flow: column nowrap;\n}\n.wrapper h2 {\n  align-self: center;\n  margin-top: 2rem;\n  margin-bottom: 2rem;\n}\n.testimonials {\n  // flex-flow: row nowrap;\n  // gap: $testimonials-gap;\n  // align-items: center;\n  // justify-content: flex-start;\n  // justify-content: center;\n  // overflow: hidden;\n  width: $testimonials-width;\n  align-self: center;\n}\n.testimonial {\n  border-radius: 15%;\n  padding: $testimonial-padding;\n  color: $quaternary-color;\n  background-color: $secondary-color;\n  width: $testimonial-width;\n  height: $testimonial-height;\n  cursor: pointer;\n  transition: transform 0.5s ease;\n}\n// .v-enter-active,\n// .v-leave-active,\n// .v-move {\n//   transition: all 1s ease;\n// }\n// .v-enter-from,\n// .v-leave-to {\n//   transform: translateX($testimonials-width/3);\n// }\n// .v-leave-active {\n//   // position: absolute;\n// }\n.carousel__slide--active .testimonial {\n  background-color: $primary-color;\n  width: calc($testimonial-width + 25px);\n  height: calc($testimonial-height + 25px);\n  font-weight: bold;\n}\n.testimonial-heading {\n  display: flex;\n  flex-flow: row nowrap;\n  align-items: center;\n  gap: 20px;\n}\n.profile-photo {\n  background-color: gray;\n  border-radius: 50%;\n  width: 80px;\n  height: 80px;\n}\n.name {\n  margin: 0 10px;\n}\n</style>\n"],"mappings":"AA0BA,SAASA,GAAG,QAAQ,KAAK;AACzB,SAASC,KAAK,QAAQ,WAAW;AACjC,SAASC,QAAQ,EAAEC,KAAK,QAAQ,eAAe;AAC/C,OAAO,iCAAiC;;;;;;;IAExC,MAAMC,MAAM,GAAGJ,GAAG,CAACC,KAAK,CAACI,YAAY,CAAC;IACtC,MAAMC,QAAQ,GAAGN,GAAG,CAAC,IAAI,CAAC;IAC1B;IACA;IACA;IACA;IACA;IACA;IACA;IACA;;IAEA;IACA;IACA;IACA;IACA;IACA;IACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}