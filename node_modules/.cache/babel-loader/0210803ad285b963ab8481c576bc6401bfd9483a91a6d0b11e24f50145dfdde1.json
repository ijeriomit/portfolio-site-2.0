{"ast":null,"code":"import FloatingHeader from \"./components/floating-header.vue\";\nimport HomeSection from \"./sections/home.vue\";\n// import Footer from \"./components/footer.vue\";\nimport gradientBackground from \"@/assets/background-gradient.svg\";\nimport whiteMatrixBackground from \"@/assets/white-matrix-background.png\";\nexport default {\n  name: \"portfolio-app\",\n  components: {\n    \"floating-header\": FloatingHeader,\n    \"home-section\": HomeSection\n    // \"footer-section\": Footer,\n  },\n  data: function () {\n    return {\n      whiteMatrixBackground,\n      gradientBackground,\n      headerColorWhite: true\n    };\n  },\n  changeHeaderColorOnScroll: function (entries) {\n    entries.forEach(entry => {\n      if (entry.isIntersecting) {\n        console.log(\"is intersecting\");\n        this.headerColorWhite = !this.headerColorWhite;\n      }\n    });\n  }\n};","map":{"version":3,"names":["FloatingHeader","HomeSection","gradientBackground","whiteMatrixBackground","name","components","data","headerColorWhite","changeHeaderColorOnScroll","entries","forEach","entry","isIntersecting","console","log"],"sources":["/Users/ijeriomitogun/Projects/TypescriptProjects/portfolio-site-2.0/src/App.vue"],"sourcesContent":["<template>\n  <div id=\"app\">\n    <div class=\"content-grid\">\n      <floating-header class=\"header\" ref=\"header\"></floating-header>\n      <div class=\"title-background\">\n        <img src=\"@/assets/matrix-background.svg\" />\n      </div>\n      <div id=\"HOME\" class=\"content-block home-section\">\n        <home-section></home-section>\n      </div>\n      <div class=\"gradient-background\"></div>\n      <div id=\"ABOUT-ME\" class=\"content-block about-section\">\n        <about-section class=\"section-wrapper\"> </about-section>\n      </div>\n      <div id=\"PROJECTS\" class=\"content-block\" style=\"grid-row: 3/3\">\n        <!-- <projects-section class=\"section-wrapper\"></projects-section> -->\n      </div>\n      <div\n        id=\"CONTACT-ME\"\n        class=\"content-block\"\n        style=\"grid-row: 5/5; flex-flow: column nowrap\"\n      >\n        <!-- <section-title>\n          <template v-slot:section-title-content>\n            <div class=\"section-title-keyword-1-alt\">await</div>\n            <div class=\"section-title-keyword-2-alt\">fetch</div>\n            <div class=\"section-title-operator-alt\">(</div>\n            <div class=\"section-title-title-alt\">Contact Me</div>\n            <div class=\"section-title-operator-alt\">)</div>\n          </template>\n        </section-title> -->\n        <!-- <contact-me class=\"section-wrapper\"></contact-me> -->\n      </div>\n      <div class=\"footer\" style=\"grid-row: 5/5\"></div>\n      <!-- <div class=\"content-block\" style=\"grid-row: 6/6\">\n        <footer-section class=\"section-wrapper\"></footer-section>\n      </div> -->\n    </div>\n    <!-- <div class=\"social-bar\"></div> -->\n  </div>\n</template>\n<script>\nimport FloatingHeader from \"./components/floating-header.vue\";\nimport HomeSection from \"./sections/home.vue\";\n// import Footer from \"./components/footer.vue\";\nimport gradientBackground from \"@/assets/background-gradient.svg\";\nimport whiteMatrixBackground from \"@/assets/white-matrix-background.png\";\nexport default {\n  name: \"portfolio-app\",\n  components: {\n    \"floating-header\": FloatingHeader,\n    \"home-section\": HomeSection,\n    // \"footer-section\": Footer,\n  },\n  data: function () {\n    return {\n      whiteMatrixBackground,\n      gradientBackground,\n      headerColorWhite: true,\n    };\n  },\n  changeHeaderColorOnScroll: function (entries) {\n    entries.forEach((entry) => {\n      if (entry.isIntersecting) {\n        console.log(\"is intersecting\");\n        this.headerColorWhite = !this.headerColorWhite;\n      }\n    });\n  },\n};\n</script>\n<style lang=\"scss\">\n@import \"@/scss/variables.scss\";\n@import \"@/scss/styles.scss\";\n\n$header-height: 100px;\n$title-height: 650px;\n$aboutMe-height: 1800px;\n$projects-height: 1600px;\n$contactMe-height: 1500px;\n$footer-height: 500px;\n\n$page-height: $title-height + $aboutMe-height + $projects-height +\n  $contactMe-height + $footer-height;\n\n#app {\n  height: $page-height;\n  width: 100vw;\n  position: relative;\n}\n.header {\n  height: $header-height;\n  position: fixed;\n  z-index: 1000;\n  grid-column: 2/2;\n}\n.footer {\n  z-index: 1;\n  position: relative;\n  background-color: $primary-color;\n  grid-column: 1/4;\n  border-top: 5px solid $secondary-color;\n}\n\n.content-grid {\n  position: relative;\n  height: 100%;\n  width: 100vw;\n  display: grid;\n  grid-template-columns: 2.5% 95% 2.5%;\n  grid-template-rows:\n    $title-height\n    $aboutMe-height\n    $projects-height\n    $contactMe-height\n    $footer-height;\n}\nhtml {\n  scroll-behavior: smooth;\n}\nbody {\n  margin: 0;\n  overflow-x: hidden;\n  overflow-y: scroll;\n}\n.background {\n  z-index: 1;\n  height: 100%;\n  position: relative;\n  grid-column: 1/4;\n  display: flex;\n  flex-flow: row;\n}\n.home-section {\n  grid-row: 1/2;\n}\n.about-section {\n  grid-row: 2/3;\n}\n.content-block {\n  z-index: 2;\n  display: flex;\n  flex-flow: row nowrap;\n  justify-content: center;\n  grid-column: 2/2;\n  overflow: hidden;\n}\n\n.title-background {\n  @extend .background;\n  background-color: $primary-color;\n  grid-row: 1 / 2;\n  width: 100%;\n  opacity: 40%;\n  display: flex;\n  overflow: hidden;\n}\n.title-background img {\n  align-self: center;\n  width: 100%;\n  height: 200%;\n}\n.gradient-background {\n  @extend .background;\n  grid-row: 2/4;\n  background-image: linear-gradient(\n    rgba(0, 135, 83, 0.63),\n    rgba(0, 135, 83, 0.275),\n    rgba(0, 135, 83, 0.1),\n    rgba(0, 135, 83, 0.1),\n    rgba(0, 135, 83, 0.275),\n    rgba(0, 135, 83, 0.63)\n  );\n}\n\n@media screen and (max-width: $phone-screen-width) {\n  .content-grid {\n    position: relative;\n    // top: 60px;\n    height: 100%;\n    width: 100vw;\n    display: grid;\n    grid-template-columns: 5% 90% 5%;\n    grid-template-rows:\n      $title-height/2\n      $aboutMe-height\n      $projects-height\n      $contactMe-height\n      $footer-height;\n  }\n}\nbody::-webkit-scrollbar {\n  display: none;\n}\n</style>\n"],"mappings":"AA0CA,OAAOA,cAAa,MAAO,kCAAkC;AAC7D,OAAOC,WAAU,MAAO,qBAAqB;AAC7C;AACA,OAAOC,kBAAiB,MAAO,kCAAkC;AACjE,OAAOC,qBAAoB,MAAO,sCAAsC;AACxE,eAAe;EACbC,IAAI,EAAE,eAAe;EACrBC,UAAU,EAAE;IACV,iBAAiB,EAAEL,cAAc;IACjC,cAAc,EAAEC;IAChB;EACF,CAAC;EACDK,IAAI,EAAE,SAAAA,CAAA,EAAY;IAChB,OAAO;MACLH,qBAAqB;MACrBD,kBAAkB;MAClBK,gBAAgB,EAAE;IACpB,CAAC;EACH,CAAC;EACDC,yBAAyB,EAAE,SAAAA,CAAUC,OAAO,EAAE;IAC5CA,OAAO,CAACC,OAAO,CAAEC,KAAK,IAAK;MACzB,IAAIA,KAAK,CAACC,cAAc,EAAE;QACxBC,OAAO,CAACC,GAAG,CAAC,iBAAiB,CAAC;QAC9B,IAAI,CAACP,gBAAe,GAAI,CAAC,IAAI,CAACA,gBAAgB;MAChD;IACF,CAAC,CAAC;EACJ;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}