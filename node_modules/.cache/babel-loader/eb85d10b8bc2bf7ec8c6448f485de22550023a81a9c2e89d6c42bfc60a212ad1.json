ont-weight: bold;\n  text-transform: uppercase;\n  font-size: 30px;\n  cursor: pointer;\n  background-color: #8f9391;\n}\n.button-row button.active {\n  background-color: $quaternary-color;\n  color: $secondary-color;\n}\n.button-row button:hover:not(.active) {\n  width: 210px;\n  height: 85px;\n  transition: all 0.1s ease-in;\n  background-color: $secondary-color;\n  color: $quaternary-color;\n}\n.headshot {\n  width: 700px;\n  height: 900px;\n}\n.headshot-2 {\n  @extend .headshot;\n}\n.point {\n  position: relative;\n  left: 90px;\n  // top: 35px;\n  width: 0;\n  height: 0;\n  border-left: 15px solid transparent;\n  border-right: 15px solid transparent;\n  border-top: 15px solid $quaternary-color;\n  align-self: flex-end;\n}\n\n@media screen and (max-width: $small-screen-width) {\n  .content-section {\n    flex-flow: column nowrap;\n    justify-content: space-evenly;\n  }\n  .text-content {\n    width: 100%;\n    max-height: 45%;\n    overflow: scroll;\n    word-break: break-word;\n    text-align: center;\n    margin-top: 0;\n    font-size: $phone-text-size;\n    order: 1;\n  }\n  .image-frame {\n    align-self: center;\n    width: $phone-image-width + 1rem;\n    order: 0;\n    height: $phone-image-height + 1rem;\n  }\n  .image-box {\n    width: 100%;\n    height: 100%;\n  }\n}\n@media screen and (max-width: $phone-screen-width) {\n}\n</style>\n"],"mappings":";OAE0BA,UAA4C;;;EAD3DC,KAAK,EAAC;AAAkB;gEAC/BC,mBAAA,CAAqE;EAAhED,KAAK,EAAC,UAAU;EAACE,GAA4C,EAA5CH;;;EAMfC,KAAK,EAAC;AAAY;gEAuBvBC,mBAAA,CAAyB;EAApBD,KAAK,EAAC;AAAO;gEAClBC,mBAAA,CAIK;EAJDD,KAAK,EAAC;AAAS,I,aACjBC,mBAAA,CAAmB,cAAb,QAAM,G,aACZA,mBAAA,CAAuB,cAAjB,cAAU,G,aAChBA,mBAAA,CAAe,cAAT,IAAE,E;gEAEVA,mBAAA,CAA2C;EAAxCD,KAAK,EAAC;AAA6B;;uBApC1CG,mBAAA,CAsCU,WAtCVC,UAsCU,GArCRC,UAAqE,EACrEC,mBAAA,+GAGM,EACNL,mBAAA,CA+BM,cA9BJA,mBAAA,CAsBM,OAtBNM,UAsBM,GArBJN,mBAAA,CAMS;IALNO,OAAK,EAAAC,MAAA,QAAAA,MAAA,MAAAC,MAAA,IAAEC,MAAA,CAAAC,cAAc,CAACF,MAAM;IAC7BG,EAAE,EAAC,OAAO;IACTb,KAAK,EAZhBc,eAAA;MAAAC,MAAA,EAY4BJ